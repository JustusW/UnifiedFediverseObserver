{% extends "base.jinja2" %}
{% block content %}
    <div id="{{ data.actor }}">Actor: {{ data.actor }}</div>
    {% for k in data.data.keys() %}
        {% if k in ['to', 'cc'] %}
            {{ k }}:
            <ul>
            {% for item in data[k] %}
                <li id="{{ item }}">{{ item }}</li>
            {% endfor %}
            </ul>
        {% endif %}
    {% endfor %}
    {{ data.object }}
{% endblock %}
